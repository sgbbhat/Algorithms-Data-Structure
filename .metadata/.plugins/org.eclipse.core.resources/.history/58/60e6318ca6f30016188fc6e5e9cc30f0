#ifndef QUEUE_H_
#define QUEUE_H_
#define QUEUE_SIZE 10

class Queue {
private:
	int queue[];
	int front = -1;
	int rear = -1;

public:
	Queue(int size);
	virtual ~Queue();
	bool IsEmpty();
	void Enqueue(int value);
	void Dequeue();
};

#endif /* QUEUE_H_ */
