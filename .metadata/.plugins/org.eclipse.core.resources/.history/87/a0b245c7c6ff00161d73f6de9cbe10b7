#include <iostream>
using namespace std;

class LinkedList
{
private:
	typedef struct node
	{
		int data;
		node * next;
	} * nodeptr;

	nodeptr currptr;
	nodeptr headptr;
	nodeptr tempptr;
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
